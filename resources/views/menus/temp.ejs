        <script>
            const label = ['5★', '4★', '3★', '2★', '1★'];
            let ratingArray = '<%= ratingArray %>';
            ratingArray = ratingArray.split(',');
            const data = {
                labels: label,
                datasets: [{
                    label: '',
                    data: ratingArray,
                    backgroundColor: [
                        '#388e3c',
                        '#388e3c',
                        '#388e3c',
                        '#ff9f00',
                        '#ff6161'
                    ],
                    borderRadius: '30',
                    borderWidth: 0,
                    barPercentage: 0.25,
                    borderSkipped: false,
                }]
            };
            // progressBar plugi{n Block
            const progressBar = {
                id: 'progressBar',
                beforeDatasetsDraw(chart, args, pluginOption) {
                    const { ctx, data, chartArea: { top, bottom, left, right, width, height }, scales: { x, y } } = chart;
                    ctx.save();
                    // labeltext 
                    ctx.font = '12px sans-serif';
                    ctx.fillStyle = 'rgba(102,102,102,1)';

                }
            }

            const ctx = document.getElementById('myChart').getContext('2d');
            const myChart = new Chart(ctx, {
                type: 'bar',
                data,
                options: {

                    maintainAspectRatio: false,
                    indexAxis: 'y',

                    plugins: {
                        tooltip: {
                            enabled: false,
                        },
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        x: {
                            beginAtZero: true,
                            grid: {
                                borderWidth: 0,
                                display: false
                            },
                            ticks: {
                                display: false
                            }

                        },
                        y: {
                            grid: {
                                borderWidth: 0,
                                display: false
                            },
                        }
                    },


                },
                plugins: [progressBar]
            });


            function shareLink() {
                var copyText = window.location.href;

                navigator.clipboard.writeText(copyText);
                alert("Product Link Copied: " + copyText);
            }




            function heartClicked() {
                document.body.classList.toggle('heartliked');
                setTimeout(() => {
                    alert("Product added to your wishlist");

                }, 400);
            }
            function starclick() {
                console.log("star clicked");
            }

            $('.ratings_star').hover(

                function () {
                    $(this).addClass('ratings_over');
                    $(this).prevAll().addClass('ratings_over');

                },

                function () {
                    $(this).removeClass('ratings_over');
                    $(this).prevAll().removeClass('ratings_over');

                }
            );
            $('.ratings_star').click(function () {
                $(this).addClass('ratings_clicked');
                $(this).prevAll().addClass('ratings_clicked');
                $(this).nextAll().removeClass('ratings_clicked');
                var stars = this.id.replace('star_', '');
                console.log(stars);
                $('#starRating').val(stars);

                switch (this.id) {
                    case 'star_1':
                        $('#ratingDesc').html('<span class="text-red-500">Very Bad</span>');
                        break;
                    case 'star_2':
                        $('#ratingDesc').html('<span class="text-red-400">Bad</span>');
                        break;
                    case 'star_3':
                        $('#ratingDesc').html('<span class="text-green-400">Good</span>');
                        break;
                    case 'star_4':
                        $('#ratingDesc').html('<span class="text-green-500">Very Good</span>');
                        break;
                    case 'star_5':
                        $('#ratingDesc').html('<span class="text-green-600">Excellent</span>');
                        break;
                }
            });

            function alertToLogIn() {

                swal({
                    title: "Register/Login",
                    text: "Authorize user have permission to place order.",
                    buttons: {
                        Register: {
                            text: "Register",
                            value: "/register",
                            className: "swal-icon5",
                        },
                        Login: {
                            text: "Login",
                            value: "/login",
                            className: "swal-icon5",
                            html: true
                        },
                        Login1: {
                            text: "Hidden Button",
                            className: "swal-icon1",
                        },
                    },
                    icon: "info",
                }).then(function (value) {
                    if (value == "login") {
                        window.location = value;
                    } else {
                        window.location = value;
                    }
                });
            }


            $('#add-cart-btn').click(function () {
                var params = {
                    pid: '<%= product._id %>',
                    qty: $('#product-qty').val(),
                    total: '<%= product.price %>',
                    vendor: '<%= product.vendor %>'
                }

                $.ajax({
                    type: 'POST',
                    url: '/update-cart',
                    data: params,
                    dataType: 'json',
                    encode: true
                }).done(function (result) {
                    if (result.status == "success") {

                        $('#cartcount').html('<super><span id = "cartcount"><i class = "fa fa-circle" style= "font-size: 10px;color: orange"></i></span></super>');
                        alert('Product added to cart');
                        $("#buttonSection").load(window.location.href + " #buttonSection");
                    }
                })
            })
            $('#saveforlater').click(function () {
                var params = {
                    vid: '<%= product._id %>',
                }

                $.ajax({
                    type: 'POST',
                    url: '/saveForLater',
                    data: params,
                    dataType: 'json',
                    encode: true
                }).done(function (result) {
                    if (result.status == "success") {
                        alert("Done")
                    }
                })
            })
            $('#buy-now-btn').click(function () {
                var params = {
                    pid: '<%= product._id %>',
                    qty: $('#product-qty').val(),
                    total: '<%= product.price %>',
                    vendor: '<%= product.vendor %>'
                }

                $.ajax({
                    type: 'POST',
                    url: '/update-cart',
                    data: params,
                    dataType: 'json',
                    encode: true,
                }).done(function (result) {
                    if (result.status == "success") {
                        $('#cartcount').html('<super><span id = "cartcount"><i class = "fa fa-circle" style= "font-size: 10px;color: orange"></i></span></super>');

                    }
                })
                var url = '/customer/checkout';
                document.write("");
                setTimeout(function () { window.location = url; }, 500);

            })


        </script>




