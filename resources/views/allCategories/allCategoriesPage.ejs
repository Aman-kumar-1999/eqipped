<h1 class="heading">Categories</h1>
<section class="container mx-auto p-4 sm:p-8 grid grid-cols-2 sm:grid-cols-5 sm:gap-2 justify-around">
    <% nashta.forEach(function(nashta){ %>
        <a href="/Category/<%= nashta.pcategory%>/<%= nashta._id%>/1">
            <div class="flex flex-col h-min w-40 p-1 border-box bg-white rounded sm:m-5 cursor-pointer">
                <lottie-player src="<%= nashta.pimage %>"  background="transparent"  speed="1"  style="width: 150px; height: 150px;"  loop  autoplay></lottie-player>
                <div class="flex border-box p-1 flex-col">
                    <div class="text-center">
                        <%= nashta.pcategory %>
                    </div>
                </div>
            </div>
        </a>
        <% }) %>

</section>



