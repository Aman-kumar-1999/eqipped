<section class="menu container mx-auto py-8 min-h-full">

    <div class="container mx-auto pt-12">
        <h1 class="font-bold text-lg mb-4">All Discount Details</h1>
        <table class="w-full table-auto bg-white">
            <thead>
                <tr>
                    <th class="px-4 py-2 text-left">Name</th>
                    <th class="px-4 py-2 text-left">Discount </th>
                    <th class="px-4 py-2 text-left">Email</th>
                    <th class="px-4 py-2 text-left">Discount Code</th>
                    <th class="px-4 py-2 text-left">Coupon Type</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(function(data){ %>

                    <tr>
                        <td class="border px-4 py-2">
                            <%= data.name %>
                        </td>
                        <td class="border px-4 py-2">
                            <%= data.discount %> %
                        </td>
                        <td class="border px-4 py-2">
                          <%= data.email %>
                        </td>
                        <td class="border px-4 py-2">
                            <%= data.disCode %>
                        </td>
                        <td class="border px-4 py-2">
                            <%= data.couponType %>
                        </td>
                        
        
                    </tr>

                <% }) %>
          

            </tbody>
        </table>
    </div>

    
